        "next_url": { "type": "string" },
        "count": { "type": "integer" }
      }
    },
    "data": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["id", "company_id", "type", "delete", "hidden"],
        "properties": {
          "id": { "type": "string" },
          "creator": { "type": "string" },
          "creator_company_user_id": { "type": "string" },
          "company_id": { "type": "string" },
          "salary_breakup_id": { "type": "string" },
          "party_company_user_id": { "type": "string" },
          "type": { "type": "string" },
          "punch_effect": { "type": "integer" },
          "project_ids": {
            "type": "array",
            "items": { "type": "string" }
          },
          "track_project_ids": {
            "type": "array",
            "items": { "type": "string" }
          },
          "delete": { "type": "integer" },
          "created": { "type": "string" },
          "updated": { "type": "string" },
          "hidden": { "type": "integer" },

          "monkey_patch_source": {
            "type": ["object", "null"]
          },

          "monkey_patch_party_company_user": {
            "type": ["object", "null"],
            "properties": {
              "id": { "type": "string" },
              "company_id": { "type": "string" },
              "company_role_id": { "type": "string" },
              "role": { "type": "string" },
              "type": { "type": "string" },
              "creator": { "type": "string" },
              "name": { "type": "string" },
              "mobile": { "type": "number" },
              "mobile_verified": { "type": "integer" },
              "user_id": { "type": "string" },
              "gstin": { "type": "string" },
              "party_id": { "type": "string" },
              "legal_business_name": { "type": "string" },
              "billing_address": { "type": "string" },
              "hidden": { "type": "integer" },
              "profile_pic": { "type": "string" },
              "email": { "type": "string" },

              "monkey_patch_company_role": {
                "type": "object"
              },
              "monkey_patch_user": {
                "type": "object"
              }
            }
          },

          "monkey_patch_attendance": {
            "type": ["object", "null"],
