          "monkey_patch_salary_breakup": {
            "type": ["object", "null"],
            "properties": {
              "id": { "type": "string" },
              "template_id": { "type": "string" },
              "payroll_id": { "type": "string" },
              "company_id": { "type": "string" },
              "type": { "type": "string" },
              "ctc_amount": { "type": "number" },
              "basic_salary": { "type": "number" },
              "gross_salary": { "type": "number" },
              "net_salary": { "type": "number" },
              "delete": { "type": "integer" }
            }
          },

          "monkey_patch_workforce": {
            "type": ["object", "null"],
            "properties": {
              "id": { "type": "string" },
